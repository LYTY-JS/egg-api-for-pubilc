{
  "name": "egg-api",
  "version": "1.0.0",
  "description": "Image processing API with QQ number usage statistics",
  "main": "egg-api.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node egg-api.js",
    "dev": "nodemon egg-api.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "win-install": "set OPENCV4NODEJS_DISABLE_AUTOBUILD=1 && npm install",
    "linux-install": "export OPENCV4NODEJS_DISABLE_AUTOBUILD=1 && npm install"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "express": "^4.19.2",
    "fluent-ffmpeg": "^2.1.3",
    "fs": "0.0.1-security",
    "jimp": "^0.22.12",
    "opencv4nodejs": "^5.6.0",
    "serve-static": "^1.14.1",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "opencv4nodejs": {
    "disableAutoBuild": 1,
    "opencvIncludeDir": "/usr/local/include/opencv4",
    "opencvLibDir": "/usr/local/lib",
    "opencvBinDir": "/usr/local/bin"
  },
  "repository": {
    "type": "git",
    "url": "YOUR_REPO_URL"
  },
  "keywords": [
    "image",
    "processing",
    "api"
  ],
